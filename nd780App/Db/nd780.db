record(bo, "Rst-Cmd") {
    field(DESC, "Reset device")
    field(DTYP, "stream")
    field(ZNAM, "0")
    field(ONAM, "1")
    field(OUT,  "@nd780.proto cmd_reset nd780")
}

record(seq, "chEU-Cmd") {
    field(DESC, "Toggle mm/pol EU")
    field(SELM, "All")

    field(DLY0, "0.5")
    field(DOL0, "1")
    field(LNK0, "KeyC CP")

    field(DLY1, "0.5")
