##########################--Monitoring--###############################
#Trigger I/O Interruption in other monitoring records every .1 sec
record(ai, "Pos-Aux") {
    field(DESC, "Auxiliar record to trigger I/O interruption")
    field(DTYP, "stream")
    field(SCAN, ".1 second")
    field(INP,  "@nd780.proto get_positions nd780")
}

record(ai, "PosX-Mon") {
    field(DESC, "Get X axis position")
    field(DTYP, "stream")
    field(SCAN, "I/O Intr")
    field(INP,  "@nd780.proto get_positionX nd780")
}

record(ai, "PosY-Mon") {
    field(DESC, "Get Y axis position")
    field(DTYP, "stream")
    field(SCAN, "I/O Intr")
    field(INP,  "@nd780.proto get_positionY nd780")
}

